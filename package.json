	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^20.11.26",
		"eslint": "^8.57.0",
		"nodemon": "^3.1.0",
		"prettier": "^3.2.5",
